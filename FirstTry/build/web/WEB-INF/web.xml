<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Account.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginPartial</servlet-name>
        <servlet-class>Account.LoginPartial</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOffController</servlet-name>
        <servlet-class>Account.LogOffController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/Account/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPartial</servlet-name>
        <url-pattern>/Account/LoginPartial</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOffController</servlet-name>
        <url-pattern>/Account/LogOffController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
